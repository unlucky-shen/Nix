PS1=' > '

alias vi='nvim'
alias historycw='history -c && history -w'
alias gc='git clone'
alias ga='git add'
alias gcm='git commit'
alias gp='git push'
alias gpl='git pull'
alias buatbalik='sudo nixos-rebuild switch'
alias upug='sudo nixos-rebuild switch --upgrade'
alias listgen='sudo nix-env -p /nix/var/nix/profiles/system --list-generations'
alias deletegen='sudo nix-env -p /nix/var/nix/profiles/system --delete-generations' # Followed by Generation no. to be deleted
alias delprevgen='sudo nix-env -p /nix/var/nix/profiles/system --delete-generations old'
alias delncg='sudo nix-collect-garbage -d'
alias nvrun='nvidia-run'
